{
    "manifest_version": 3,
    "name": "Portkey",
    "version": "1.0",
    "description": "Show bookmarks on new tab page",
    "permissions": [
      "bookmarks",
      "favicon"
    ],
    "host_permissions": [
      "<all_urls>"
    ],
    "chrome_url_overrides": {
      "newtab": "newtab.html"
    },
    "icons": {
      "48": "img/icon48.png",
      "128": "img/icon128.png"
    }
  }